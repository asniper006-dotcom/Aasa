    {Name="Decal", Text="Decal", Position=UDim2.new(1.01646,0,0.42969,0), Size=UDim2.new(0.30453,0,0.21094,0), BackgroundTransparency=0, BorderColor3=Color3.fromRGB(0,0,0)}
