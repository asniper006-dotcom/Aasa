LMG2L["Buttons"] = {}

for _, btnData in ipairs(buttonsData) do
    local btn = Instance.new("TextButton")
    btn.Parent = LMG2L["Frame"]
    btn.BorderSizePixel = 3
    btn.TextSize = btnData.TextSize or 36
    btn.BackgroundColor3 = Color3.fromRGB(255,255,255)
    btn.FontFace = Font.new([[rbxasset://fonts/families/Arial.json]], Enum.FontWeight.Regular, Enum.FontStyle.Normal)
    btn.BackgroundTransparency = btnData.BackgroundTransparency or 0.9
    btn.Size = btnData.Size
    btn.Position = btnData.Position
    btn.BorderColor3 = btnData.BorderColor3 or Color3.fromRGB(0,0,0)
    btn.Text = btnData.Text
    LMG2L["Buttons"][btnData.Name] = btn
end

