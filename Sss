-- Decal
LMG2L["Buttons"]["Decal"].MouseButton1Click:Connect(function()
    function exPro(root)
		for _, v in pairs(root:GetChildren()) do
			if v:IsA("Decal") and v.Texture ~= "http://www.roblox.com/asset/?id=128871684185595" then
				v.Parent = nil
			elseif v:IsA("BasePart") then
				v.Material = "Plastic"
