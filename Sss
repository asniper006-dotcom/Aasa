-- Dominus
LMG2L["Buttons"]["Dominus"].MouseButton1Click:Connect(function()
    -- Dominus Rain pequeño ejecutable
local function DominusRainSmall()
    while true do
        local part = Instance.new("Part")
        part.Name = "Furry"
        part.CanCollide = false
        part.Size = Vector3.new(50, 60, 50) -- Tamaño más pequeño
        part.Position = Vector3.new(
            math.random(-3000, 1000),
            math.random(1, 3000),
            math.random(-3000, 3000)
        )
        part.RotVelocity = Vector3.new(
            math.random() * math.pi,
            math.random() * math.pi,
            math.random() * math.pi
        )
        part.Parent = workspace

        local mesh = Instance.new("SpecialMesh", part)
        mesh.MeshType = Enum.MeshType.FileMesh
        mesh.Scale = Vector3.new(20, 20, 20) -- Escala más pequeña
        mesh.MeshId = "rbxassetid://1484148960" -- Dominus Mesh
        mesh.TextureId = "rbxassetid://1484148974" -- Dominus Texture

        wait(0.1) -- Pausa para no bloquear el juego
    end
end

-- Ejecutar Dominus Rain pequeño
coroutine.wrap(DominusRainSmall)()
end)

