-- =========================
-- GUI CREATION
-- =========================

local LMG2L = {}

-- ScreenGui
LMG2L["ScreenGui"] = Instance.new("ScreenGui")
LMG2L["ScreenGui"].Parent = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")
LMG2L["ScreenGui"].ZIndexBehavior = Enum.ZIndexBehavior.Sibling

-- Frame
LMG2L["Frame"] = Instance.new("Frame")
LMG2L["Frame"].Parent = LMG2L["ScreenGui"]
LMG2L["Frame"].BorderSizePixel = 3
LMG2L["Frame"].BackgroundColor3 = Color3.fromRGB(255, 255, 255)
LMG2L["Frame"].Size = UDim2.new(0.45962, 0, 0.57805, 0)
LMG2L["Frame"].Position = UDim2.new(0.24589, 0, 0.13096, 0)
LMG2L["Frame"].BorderColor3 = Color3.fromRGB(0, 0, 0)

-- ImageLabel
LMG2L["ImageLabel"] = Instance.new("ImageLabel")
LMG2L["ImageLabel"].Parent = LMG2L["Frame"]
LMG2L["ImageLabel"].BorderSizePixel = 0
LMG2L["ImageLabel"].BackgroundColor3 = Color3.fromRGB(255, 255, 255)
LMG2L["ImageLabel"].Image = "rbxassetid://100091122003498"
LMG2L["ImageLabel"].Size = UDim2.new(1, 0, 0.80469, 0)
LMG2L["ImageLabel"].Position = UDim2.new(0, 0, 0.19531, 0)

-- TextLabel
LMG2L["TextLabel"] = Instance.new("TextLabel")
LMG2L["TextLabel"].Parent = LMG2L["Frame"]
LMG2L["TextLabel"].BorderSizePixel = 3
LMG2L["TextLabel"].TextSize = 48
LMG2L["TextLabel"].BackgroundColor3 = Color3.fromRGB(255, 255, 255)
LMG2L["TextLabel"].FontFace = Font.new([[rbxasset://fonts/families/Arial.json]], Enum.FontWeight.Regular, Enum.FontStyle.Normal)
LMG2L["TextLabel"].Size = UDim2.new(1, 0, 0.19531, 0)
LMG2L["TextLabel"].BorderColor3 = Color3.fromRGB(0, 0, 0)
LMG2L["TextLabel"].Text = "†††† g00by gui †††††"

-- UIDragDetector
LMG2L["UIDragDetector"] = Instance.new("UIDragDetector")
LMG2L["UIDragDetector"].Parent = LMG2L["Frame"]

-- UIAspectRatioConstraint
LMG2L["UIAspectRatioConstraint"] = Instance.new("UIAspectRatioConstraint")
LMG2L["UIAspectRatioConstraint"].Parent = LMG2L["Frame"]
LMG2L["UIAspectRatioConstraint"].AspectRatio = 1.89844

